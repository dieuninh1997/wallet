{
  "name": "MangoWallet",
  "version": "0.0.1",
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  },
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios-6": "node node_modules/react-native/local-cli/cli.js run-ios --simulator \"iPhone 6\"",
    "ios-x": "node node_modules/react-native/local-cli/cli.js run-ios --simulator \"iPhone X\"",
    "build:android": "rm -rf ./android/build && cd android && ./gradlew assembleRelease",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/wallet/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "shake": "adb shell input keyevent 82",
    "postinstall": "cd nodejs-assets/nodejs-project/ && npm install",
    "kill": "sudo kill $(lsof -t -i:8081)"
  },
  "dependencies": {
    "@haskkor/react-native-pincode": "https://github.com/Sotatek-AnhNguyen/react-native-pincode.git",
    "axios": "0.18.0",
